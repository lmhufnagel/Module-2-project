<%= form_for @user do |f|%>
Username: <%= f.text_field :name %><br>
Bio: <%= f.text_area :bio %> <br>
Favorite Food: <%= f.text_field :favorite_food %><br>
Password: <%= f.password_field :password %><br>
Confirm Password: <%= f.password_field :password_confirmation %>
<%= f.submit "Sign Up" %>
<% end %>
